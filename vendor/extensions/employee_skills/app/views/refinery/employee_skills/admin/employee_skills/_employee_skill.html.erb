<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(employee_skill) -%>">
  <span class='title'>
    <%= employee_skill.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_employee_skills_admin_employee_skill_path(employee_skill),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.employee_skills_admin_employee_skill_path(employee_skill),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => employee_skill.title),
        :method => :delete %>
  </span>
</li>
